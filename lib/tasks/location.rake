  def report_on_name(name)
    result = false
    fixed_name = LOCATION_FIXES[name]
    if fixed_name
      reasons = Location.dubious_name?(fixed_name, true, false)
    else
      reasons = Location.dubious_name?(name, true)
      reasons.push("Unrecognized location: #{name}")
    end
    if reasons.length != 0
      result = true
      print("\n    \"#{name}\" => \"#{fixed_name or name}\",\n")
      for r in reasons
        print("    \# #{r}\n")
      end
    end
    return result
  end
  
  desc "Check unfixed location names to find ones that would not be considered dubious"
  task(:unfixed => :environment) do
    count = 0
    for key in LOCATION_FIXES.keys()
      if (key != LOCATION_FIXES[key]) and not Location.dubious_name?(key, true)
        count += 1
        print("#{key} => #{LOCATION_FIXES[key]}\n")
      end
    end
    print "#{count}"
  end
  
  desc "Test"
  task(:test => :environment) do
    report_on_name("North America")
  end

  desc "List dubious locations"
  task(:dubious => :environment) do
    count = 0
    for l in Location.find(:all)
      count += 1 if report_on_name(l.name)
    end
    wheres = Observation.connection.select_values %(
      SELECT DISTINCT `where` FROM `observations`
      WHERE `where` is not NULL
      ORDER BY `where`
    )
    for w in wheres
      count += 1 if report_on_name(w)
    end
    print "Number of dubious names: #{count}\n"
  end

  desc "List locations"
  task(:list => :environment) do
    for l in Location.find(:all)
      if not Location.dubious_country?(l.name)
        w = l.name.gsub("\"", "\\\"")
        print("\"#{w}\" => \"#{w}\",\n")
      end
    end
    wheres = Observation.connection.select_values %(
      SELECT DISTINCT `where` FROM `observations`
      WHERE `where` is not NULL
      ORDER BY `where`
    )
    for w in wheres
      if not Location.dubious_country?(w)
        loc = w.gsub("\"", "\\\"")
        print("\"#{loc}\" => \"#{loc}\",\n")
      end
    end
  end

  def add_comment(object, type, from_name, to_name)
    if from_name != to_name
      loc_note = "\n[admin - #{Time.now}]: Changed location name from '#{from_name}' to '#{to_name}'\n\n"
      if object.notes
        object.notes += "\n#{loc_note}"
      else
        object.notes = loc_note
      end
      object.save
    end
  end
    
  def fix_location(current_name, current_location)
    target_name = LOCATION_FIXES[current_name]
    if target_name
      current_sql_safe = current_name.gsub(/[']/, '\\\\\'')
      target_sql_safe = target_name.gsub(/[']/, '\\\\\'')
      target_location = Location.find_by_name(target_name)
      if target_location
        obs = Observation.find_all_by_where(current_name)
        if obs.size > 0
          for o in obs
            o.where = target_name
            o.save
            add_comment(o, 'Observation', current_name, target_name)
          end
          # params[:id]Observation.connection.update("UPDATE observations SET location_id = #{target_location.id}, `where` = NULL WHERE `where` = '#{current_sql_safe}'")
          print("Moved #{obs.size} observations from non-location: #{current_name} to location: #{target_name}\n")
        end
        if current_location and (current_location != target_location)
          obs = Observation.find_all_by_location_id(current_location.id)
          if obs.size > 0
            for o in obs
              o.location = target_location
              o.where = nil
              o.save
              add_comment(o, 'Observation', current_name, target_name)
            end
            # Observation.connection.update("UPDATE observations SET location_id = #{target_location.id}, `where` = NULL WHERE location_id = #{current_location.id}")
            print("Moved #{obs.size} observations from location: #{current_name} to location: #{target_name}\n")
          end
          comments = current_location.comments
          if comments.size > 0
            for comment in comments
              comment.object_id = target_location.id
              comment.save
            end
            print("Moved #{comments.size} comments from #{current_name} to #{target_name} (#{target_location.id})\n")
          end
          descs = current_location.descriptions
          if descs.size > 0
            for d in descs
              d.location_id = target_location.id
              d.save
            end
            print("Moved #{descs.size} descriptions from #{current_name} to #{target_name} (#{target_location.id})\n")
          end
          current_location.destroy
          print("Deleted #{current_name}\n")
        end
      elsif current_name != target_name
        if current_location
          current_location.name = target_name
          current_location.save
          add_comment(current_location, 'Location', current_name, target_name)
          print("Renamed location: #{current_name} to: #{target_name}\n")
        else
          obs = Observation.find_all_by_where(current_name)
          for o in obs
            o.where = target_name
            o.save
            add_comment(o, 'Observation', current_name, target_name)
          end
          print("Changed non-location: #{current_name} to: #{target_name}\n")
        end
      end
    end
  end

  desc "Fix dubious locations"
  task(:fix => :environment) do
    User.current = User.find(1) # nathan
    for l in Location.find(:all)
      fix_location(l.name, l)
    end
    wheres = Observation.connection.select_values %(
      SELECT DISTINCT `where` FROM `observations`
      WHERE `where` is not NULL
      ORDER BY `where`
    )
    for w in wheres
      fix_location(w, Location.find_by_name(w))
    end
  end

  desc "Location names that contain counties and should be reviewed"
  task(:counties => :environment) do
    candidates = {}
    for v in LOCATION_FIXES.values
      c = Location.no_dubious_county(v)
      if c and c != v
        candidates[v] = 'Fix'
      end
    end
    for k in candidates.keys.sort
      print("#{k}: #{candidates[k]}\n")
    end
  end
  
  desc "Count the number of proposed changes"
  task(:count => :environment) do
    count = 0
    for k in LOCATION_FIXES.values
      count += 1 if k != LOCATION_FIXES[k]
    end
    print("Changing #{count} out of #{LOCATION_FIXES.size}\n")
  end
