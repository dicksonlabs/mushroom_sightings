<div id="Title">New Species List</div></br>

<%= start_form_tag :action => 'create_species_list' %>
<center><%= submit_tag "Create" %></center>
<table cellpadding="10">
<tr>
	<%= render :partial => 'form_species_lists' %>
	<td valign="top">
		<p>
			<label for="species_list_species">Species</label><br/>
			<%= text_area 'species_list', 'species'  %>
		</p>
	</td>
</tr>
</table>
<%= end_form_tag %>
