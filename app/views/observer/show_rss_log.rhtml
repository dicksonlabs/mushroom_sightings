<%
  @title = :show_rss_log_title.t(:title => @rss_log.unique_format_name)

  draw_prev_next_tabs(@rss_log)
%>

<% if @rss_log.notes %>
  <table>
    <% for key, args, time in @rss_log.parse_log
      if key != :log_orphan %>
        <tr>
          <td style="white-space:nowrap"><%= time.web_time %></td>
          <td width="10"></td>
          <td><%= key.t(args) %></td>
        </tr>
      <% end
    end %>
  </table>
<% end %>

<%= if target = @rss_log.target
  link_to(:show_object.t(:type => target.type_tag),
          :controller => target.show_controller,
          :action => target.show_action, :id => target.id,
          :params => query_params)
end %>
