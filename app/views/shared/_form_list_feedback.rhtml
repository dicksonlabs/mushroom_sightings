<% if @new_names && (@new_names != []) %>
  <div style="width:500px">
    <div class="Errors">
      <%= :form_list_feedback_missing_names.l %><br/>
      <span class="HelpNote">
        <%= :form_list_feedback_missing_names_help.l %>
      </span>
      <span class="Data">
        <% for n in @new_names %>
        <br/>&nbsp;&nbsp;<%= h(n) %>
        <% end %>
      </span>
    </div>
  </div>
<% end %>
